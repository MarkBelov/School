#include <iostream>
#include <string>

using namespace std;

int first();
int second();
int third();
int fourth();
int fifth();
int sixth();

int main ()
{
int q;
cin >> q;

if (q == 1) {
    first();
} else if (q == 2) {
    second();
} else if (q == 3) {
    third();
} else if (q == 4) {
    fourth();
} else if (q == 5) {
    fifth();
} else if (q == 6) {
    sixth();
}

    return 0;
}


int first() {
    char str;
    cin >> str;
    if ((str >= 48) && (str <= 57)) {
            cout << "Yes";
    }
    else
    {
        cout << "No";
    }
    return 0;
}


int second() {
    char str;
    cin >> str;
    str -= 32;
    cout << str;

    return 0;
}


int third() {
    char str;
    cin >> str;
    if (str > 64 && str < 91) {
        str += 32;
    } else if (str > 96 && str < 123) {
        str -=32;
    }
    cout << str;

    return 0;
}


int fourth() {
    int a = 0;
    string str;
    cin.sync();
    getline(cin, str);
    for (int i = 0; i < str.length(); i++) {
    if (str[i] == ' ') {
     a++;
    }
    }
    cout << a;
    return 0;
}


int fifth() {
    int l;
    string str;
    cin.sync();
    getline(cin, str);
    l = str.length();

        for (int i = 0; i < l/2; i++) {
            if (str[i] == str[l-1-i]) {

            } else {

            cout << "NO";
            return 1;

            }
        }
    cout << "YES";
    return 0;
}


int sixth() {
string str, a, b;
	cin.sync();
	getline(cin, str);
	for (int i = 0; i < str.length(); i++) {
		if (str[i] == ' ') {
			if (a.length() > b.length()) {
				b = a;
                a = "";
			} else {
				a = "";
            }
} else {
			a  += str[i];
		}
	}
if (a.length() > b.length()) {
				b = a;
}
cout << b;
    return 0;
}
